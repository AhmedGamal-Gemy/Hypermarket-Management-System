<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="358.0" prefWidth="452.0" spacing="20.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hms.source.code.Controller.PrimaryController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <TextField fx:id="email_input" promptText="Email" />
      <TextField fx:id="password_input" promptText="Password" />
      <Button fx:id="login_btn" mnemonicParsing="false" onAction="#loginBtn" text="Login" />
      <Text fx:id="error_message" strokeType="OUTSIDE" strokeWidth="0.0" />
   </children>
</VBox>
